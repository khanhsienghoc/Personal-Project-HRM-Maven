<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Admin- Login Test Suite">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
        <listener class-name="reportConfigs.MethodListener"/>
        <listener class-name="reportConfigs.AllureTestListener"/>
        <listener class-name="retryConfigs.RetryListener"/>
    </listeners>
    <test name="Run on chrome">
        <parameter name="browser" value="chrome"/>
        <parameter name="environment" value="test"></parameter>
        <classes>
            <class name="admin.User_01_Admin_Login"/>
        </classes>
    </test>
</suite>